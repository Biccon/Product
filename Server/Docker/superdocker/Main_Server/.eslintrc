{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "env": {
        "es6": true
    },
    "rules": {
        "no-async-promise-executor": "error",
        "no-console": "error",
        "no-var": "warn",
        "prefer-const": "warn"
    }
}
